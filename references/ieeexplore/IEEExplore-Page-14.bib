  author={Irfana Parveen, C.A. and Sunder, R and Kumar, R Satheesh},
  booktitle={2022 IEEE World Conference on Applied Intelligence and Computing (AIC)}, 
  title={A Review on Deep Learning Models for Optic Disc Segmentation and Glaucoma Classification}, 
  year={2022},
  volume={},
  number={},
  pages={182-187},
  abstract={Due to the recent existence of large datasets and advancements in processing capabilities, deep learning has risen to the forefront of artificial intelligence on a variety of tasks, particularly those related to image classification and pattern recognition. Ophthalmology offers a chance to see how deep learning classifiers are used in medicine. Globally, glaucoma is the prime factor of chronic blindness and disability. Despite this, most patients are unsure whether they have glaucoma, and detecting glaucoma progression with present technology is challenging in clinical practice. We can detect glaucoma early with the help of deep learning technology. The segmentation of the optic disc and classification of glaucoma using retinal data will be examined using several deep structured learning approaches in this research. Also presented a basic understanding of deep learning. Finally, the difficulties that deep learning models face are highlighted.},
  keywords={Deep learning;Biomedical optical imaging;Face recognition;Computer architecture;Optical imaging;Retina;Ophthalmology;deep learning;segmentation;optic disk;optic cup;classification;glaucoma},
  doi={10.1109/AIC55036.2022.9848985},
  ISSN={},
  month={June},}@INPROCEEDINGS{9825385,
  author={Pawar, Digvijay J. and Kanse, Yuvraj K. and Patil, Suhas S.},
  booktitle={2022 3rd International Conference for Emerging Technology (INCET)}, 
  title={Automated Glaucoma Classification Based on Combined Features Rule Formation}, 
  year={2022},
  volume={},
  number={},
  pages={1-4},
  abstract={According to the survey carried out by the World Health Organization (WHO), globally a minimum of 2.2 billion persons are impacted with vision defacement. Despite that, almost half of them could have been prevented or have to be addressed yet. Glaucoma is thought to be the third-largest cause of visual loss and blindness. Glaucoma is one of the non-communicable visual diseases, which damages the optic nerve head. When someone is detected with an eye disorder, it can’t be dealt with entirely, but it can possible to control with timely identification and proper treatment. If glaucoma is not always identified in time, it will bring about vision impairment by means of adverse effects on the optic nerve head. The glaucoma diagnosis is achieved by the Optic Disc (OD) and Optic Cup (OC) boundaries in the retinal images, this may be recorded with a fundus camera. In the present scenario, several image processing and artificial intelligence techniques are utilized to segment and categorize the optic disc and optic cup, which are fundamental to ocular images. To intensify the exactness of early-level glaucoma identification, the prevailing methods can have modifications in different and characteristic choices of the retinal photographs. This paper helps with a different approach to RNFL and ISNT based glaucoma detection and classification of retinal images. The experimental results prove that, for different classifiers and feature combinations, the diagnostic accuracy of PNN is better than that of other classifiers used. The implemented system gives a maximum accuracy of 96.25%, sensitivity of 95.83%, and specificity of 96.67% for the PNN classifier.},
  keywords={Visualization;Head;Sensitivity;Optical recording;Visual impairment;Optical imaging;Retina;Early Detection;Classifier;RNFL;ISNT ratio},
  doi={10.1109/INCET54531.2022.9825385},
