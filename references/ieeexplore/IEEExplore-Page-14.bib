@INPROCEEDINGS{10497285,
  author={Deshmukh, Amruta Pisal and Lukade, Rehan and Pawar, Sanket and Tekade, Pallavi and Salunke, Dipmala and Rangdale, Sonali},
  booktitle={2024 International Conference on Emerging Smart Computing and Informatics (ESCI)}, 
  title={Smart Survival Lance System for Visually Impaired Person}, 
  year={2024},
  volume={},
  number={},
  pages={1-4},
  abstract={The ultimate objective of the field of computer vision is inventing software that allows computers to recognize and interpret humans and things in photos, videos, and the actual world. in accordance to a global health organization, there are approximated 2.2 billion individuals with vision impairment around the globe, with causes of blindness ranging from cataracts to age-related macular degeneration. Due to their visual impairment, these people encounter daily obstacles. Computer vision, with the use of algorithms like YOLO as well as datasets and neural networks, can significantly enhance the lives of visually impaired people. Previous research has made use of technologies such as YOLOv7, Neural Network, GTTs, dataset, and R-NCC. for this survey study is to offer an AI-based intelligent system that improves the lives of visually impaired people by recognizing items in their environment and making audible announcements for the objects that are detected. This suggested approach will make it easier for visually impaired people to navigate interior as well as exterior settings. The sole objective of the current study is to propose a sensible and effective system that addresses the challenges that visually impaired people face},
  keywords={YOLO;Surveys;Macular degeneration;Computer vision;Navigation;Neural networks;Organizations;YOLO;GTTS;COCO;GPS;TensorFlow;HOG;R-NCC},
  doi={10.1109/ESCI59607.2024.10497285},
  ISSN={},
  month={March},}@INPROCEEDINGS{10635674,
  author={Presil, Dan},
  booktitle={2024 IEEE International Symposium on Biomedical Imaging (ISBI)}, 
  title={Justraigs Competition Insights: An Ensemble of Methods for Automated Referable Glaucoma Diagnosis}, 
  year={2024},
  volume={},
  number={},
  pages={1-4},
  abstract={Glaucoma is a leading cause of irreversible vision loss and blindness worldwide. Early detection and treatment are essential for preventing visual impairment, and population-based glaucoma screening plays a key role in facilitating early identification. This paper presents a technical report on a solution for the Justified Referral in AI Glaucoma Screening (JustRAIGS) challenge. The proposed approach tackles a dual classification task, combining binary and multi-label classification using large input images. To manage data imbalance, techniques such as soft labeling, balanced weighted sampling, and image augmentations were employed. For post-processing, the solution optimizes thresholds and utilizes test-time augmentations to enhance prediction accuracy. This comprehensive approach aims to improve the efficiency and effectiveness of glaucoma screening, ultimately contributing to better patient outcomes and advancing the field of ophthalmology.Source code: github.com/danpresil/JustRAIGS-IEEE-ISBI-2024},
  keywords={Glaucoma;Codes;Visual impairment;Blindness;Writing;Image augmentation;Labeling;Glaucoma;Color Fundus Photographs;Artificial Intelligence;Explainable AI;Deep Learning},
  doi={10.1109/ISBI56570.2024.10635674},
  ISSN={1945-8452},
  month={May},}@INPROCEEDINGS{11099736,
  author={Pavithra, R. and G, Dhina Thayaalan and K, Dhanush and M, Dhinakaran},
  booktitle={2025 International Conference on Emerging Technologies in Engineering Applications (ICETEA)}, 
  title={An IoT Enabled Remote Diabetic Screening System}, 
  year={2025},
  volume={},
  number={},
  pages={1-4},
  abstract={If left untreated, diabetic retinopathy (DR), a serious side effect of diabetes, can cause blindness or vision impairment. This project presents an IoT-enabled diabetic retinopathy screening system that utilizes a CNN model for accurate classification of retinal images. The system receives pre-captured DR images from hospitals, processes them using deep learning, and provides classification results through a userfriendly Streamlit-based graphical interface. The model achieves 97% accuracy, as demonstrated in the classification report and confusion matrix. A confidence score visualization enhances interpretability, ensuring transparent decisionmaking. Additionally, a real-time performance graph tracks training and validation accuracy, confirming the model's stability. This Phase 1 implementation successfully establishes an AI-driven automated screening tool, which can assist healthcare professionals in early DR detection. Future enhancements in Phase 2 will focus on integrating an ESP32 microcontroller for real-time IoT communication, enabling direct email notifications to patients and doctors.},
  keywords={Training;Diabetic retinopathy;Visualization;Accuracy;Microcontrollers;Visual impairment;Streaming media;Retina;Real-time systems;Stability analysis;IoT-driven healthcare solutions;Diabetic Retinopathy (DR);Retinal fundus images},
  doi={10.1109/ICETEA64585.2025.11099736},
  ISSN={},
  month={June},}@INPROCEEDINGS{11064170,
  author={Kaushik, Ankur and Rawat, Bhupender Singh},
  booktitle={2025 3rd International Conference on Communication, Security, and Artificial Intelligence (ICCSAI)}, 
  title={Deep Model Exposure and Classification of Diabetic Retinopathy Using Fundus Imaging}, 
  year={2025},
  volume={3},
  number={},
  pages={1158-1161},
  abstract={Diabetic retinopathy is the most common complication of diabetes mellitus and a major cause of preventable blindness worldwide. Despite significant advances, effective treatment options remain limited, especially for proliferative diabetic retinopathy and diabetic macular edema. This paper presents a new Prognosis of Microaneurysm and Non-Proliferative Diabetic Retinopathy (PMNPDR) detection framework based on deep convolutional neural networks (CNNs) for semantic segmentation of retinal fundus images. Using integration of sophisticated technologies such as convolutional 3D layers, pooling, and activation with ReLU gives the potential feature extraction ability which reduces overfitting. By using PMNPDR architecture the accuracy comes around 99.6 % followed by 98.11 % and 99.32 % with sensitivity and specificity respectively, indicating a drastic improved performance against most of the commonly used architectures for example, AlexNet, VGGNet and GoogleNet, etc. Accurate classification of DR stages by the system has proved that deep learning models can transform early diagnosis and thus lead to timely interventions with better patient outcomes. This study underlines the importance of harnessing AI-driven solutions for superior diagnostic accuracy and efficiency in combating DR.},
  keywords={Deep learning;Diabetic retinopathy;Accuracy;Computational modeling;Visual impairment;Computer architecture;Blindness;Retina;Feature extraction;Overfitting;Diabetic retinopathy;Diabetic macular edema},
  doi={10.1109/ICCSAI64074.2025.11064170},
  ISSN={},
  month={April},}@INPROCEEDINGS{11052914,
  author={Vikraman, Lekshmi and Sumathi, R},
  booktitle={2025 International Conference on Computational Robotics, Testing and Engineering Evaluation (ICCRTEE)}, 
  title={Diabetic Retinopathy Classification Using Pyramidal Convolution Shuffle Attention Neural Network}, 
  year={2025},
  volume={},
  number={},
  pages={1-5},
  abstract={Diabetic retinopathy (DR) is the most dangerous consequences of diabetes that cause lifelong blindness. It has become more widespread in recent years. Furthermore, diabetic patients' retinal blood vessels are damaged by DR. Consequently, several DL with AI methods have been put out to automatically detect anomalies in DR, distinct phases from photographs of the retina. This study introduces DRC-PCS-ANN, a novel neural network architecture based on pyramidal convolution and shuffle attention, for effective diabetic retinopathy classification. As a first step, input images were acquired from the MESSIDOR database. Following acquisition, noise is removed from the images through pre-processing using Data-Adaptive Gaussian Average Filtering (DAGAF). In the subsequent step, statistical features like Mean, Kurtosis, Variance, and Entropy are extracted using the One-Dimensional Quantum Integer Wavelet S-Transform. Finally, the extracted features are fed into PCSANN for effective classification of Diabetic Retinopathy into five categories: No DR, Mild DR, Moderate DR, Severe DR, and Proliferative DR. Then, performance of DRC-PCS-ANN examined utilizing performance metrics like accuracy, precision, F1-score. The DRC-PCS-ANN method attains 21.28%, 21.52% and 20.34% higher accuracy, 23.29%, 23.83% and 21.72% higher precision and compared with existing DR classifiers such as DR Classification depend on Multipath CNN with ML classifiers (DRC-MCNN-MLC), Automated DR screening for Primary Care Settings utilizing DL (ADR-PCS-DL) and Automatic DR diagnosis utilizing Adaptive Fine-Tuned CNN (ADR-AFT-CNN) respectively.},
  keywords={Diabetic retinopathy;Accuracy;Convolution;Filtering;Databases;Neural networks;Noise;Feature extraction;Wavelet analysis;Retina;data-adaptive gaussian average filtering;diabetic retinopathy;messidor database;one-dimensional quantum integer wavelet s-transform;pyramidal convolution shuffle attention neural network},
  doi={10.1109/ICCRTEE64519.2025.11052914},
  ISSN={},
  month={May},}@INPROCEEDINGS{11013100,
  author={Jilan, Tahsin Zaman and Bhuiyan, Mahdi Hasan and Haldar, Sumit and Chowdhury, Maisha Shabnam and Bushra, Nazifa},
  booktitle={2025 International Conference on Electrical, Computer and Communication Engineering (ECCE)}, 
  title={An Interpretable Diagnosis of Retinal Diseases Using Vision Transformer and Grad-CAM}, 
  year={2025},
  volume={},
  number={},
  pages={1-6},
  abstract={Timely identification of retinal disorders is essential to mitigate the risk of vision impairment or total blindness. This study introduces an innovative and interpretable diagnostic framework that integrates the capabilities of a CNN-based architecture and a Transformer model, followed by visualization using Grad-CAM, to tackle the complexities of multi-label classification in retinal disease analysis. By utilizing OCT imagery, we have developed a specialized hybrid system that synergizes deep learning techniques from convolutional and transformer-based networks to enhance diagnostic accuracy. Beyond classification, this approach provides translucent insights into the decision-making process of the model through activation-based visual explanations, improving trust and interpretability in medical AI applications. The CNN-based model achieved an accuracy rate of 88.88%, while the Transformer-based approach reached 91.39%. However, our optimized hybrid configuration significantly outperformed these standalone models. With an impressive accuracy of 98.8%, this advanced system demonstrates its strong potential in revolutionizing retinal disease detection and assisting healthcare professionals in early intervention.},
  keywords={Deep learning;Visualization;Computer vision;Accuracy;Computational modeling;Visual impairment;Transformers;Retina;Medical diagnosis;Diseases;Deep Learning;Vision Transformers;Ocular diseases screening;Grad-CAM;Detection;Diagnosis;classification},
  doi={10.1109/ECCE64574.2025.11013100},
  ISSN={},
  month={Feb},}@INPROCEEDINGS{9768544,
  author={Suganyadevi, S. and Renukadevi, K. and Balasamy, K. and Jeevitha, P.},
  booktitle={2022 First International Conference on Electrical, Electronics, Information and Communication Technologies (ICEEICT)}, 
  title={Diabetic Retinopathy Detection Using Deep Learning Methods}, 
  year={2022},
  volume={},
  number={},
  pages={1-6},
  abstract={Diabetes mellitus causes diabetic retinopathy (DR), which is the primary source of blindness worldwide. Initial identification and cure are required to postpone or avert visual degradation and loss. In this regard, various artificial-intelligence-powered approaches for detecting and classifying diabetic retinopathy on fundus retina pictures have been proposed by the scientific community. This paper will mostly examine existing early DR diagnostic tools to determine their merits and drawbacks. Although pictures from fluorescein angiography, colour fundus medical images or visual lucidity tomography angiography are used for early diagnosis. Only colour fundus medical images are included in this study. It is possible to categorise the early DR detection methods described in this paper as either classical image processing, traditional machine learning, or deep learning. The issues that must be addressed in creating such efficient, effective and resilient methods for initial detection of DR systems are discussed in length in this study, as is the substantial opportunity for future research in this field.},
  keywords={Deep learning;Visualization;Retinopathy;Image color analysis;Angiography;Tomography;Retina;diabetes mellitus;diabetic retinopathy;fundus retina;machine learning;deep learning},
  doi={10.1109/ICEEICT53079.2022.9768544},
  ISSN={},
  month={Feb},}@INPROCEEDINGS{10007470,
  author={Vayadande, Kuldeep and Ingale, Varad and Verma, Vivek and Yeole, Abhishek and Zawar, Sahil and Jamadar, Zoya},
  booktitle={2022 International Conference on Signal and Information Processing (IConSIP)}, 
  title={Ocular Disease Recognition using Deep Learning}, 
  year={2022},
  volume={},
  number={},
  pages={1-7},
  abstract={Artificial intelligence holds a significant impact in a variety of drug-related medical studies, including ophthalmology. Deep literacy styles, in particular, have been successful in detecting clinical signs and bracketing optical conditions. Studies reveal Ocular diseases to be the major contributing reason of childhood blindness all over the world. Rapid and automatic illness identification is vital and urgent in lowering the strain of ophthalmologists. Ophthalmologists use pattern recognition to identify disorders by looking at the eye and its surrounding tissues directly or indirectly. As a result, can benefit the area of medical greatly. Each disease has several severity levels that can be identified by confirming the presence of different lesions. Morphological characteristics identify each lesion, and numerous lesions from different diseases have similar characteristics. In ophthalmology, deep literacy techniques have mostly been employed on eye fundus pictures and optic consonance tomography. In this paper, we have used three models namely CNN, Inception V3, VGG-19 for cataract prediction. We have got accuracy of 0.9587 for VGG-19 which is performing best as compared to other models.},
  keywords={Deep learning;Biomedical optical imaging;Sociology;Tomography;Ophthalmology;Lesions;Statistics;Clinical Diagnosis;Deep Learning;Image Classification;Neural Network;Ocular disease},
  doi={10.1109/ICoNSIP49665.2022.10007470},
  ISSN={},
  month={Aug},}@INPROCEEDINGS{9854758,
  author={Khader, Firas and Haarburger, Christoph and Kirr, Jörg-Christian and Menke, Marcel and Kather, Jakob Nikolas and Stegmaier, Johannes and Kuhl, Christiane and Nebelung, Sven and Truhn, Daniel},
  booktitle={2022 IEEE International Symposium on Biomedical Imaging Challenges (ISBIC)}, 
  title={Elevating Fundoscopic Evaluation to Expert Level - Automatic Glaucoma Detection Using Data from the Airogs Challenge}, 
  year={2022},
  volume={},
  number={},
  pages={1-4},
  abstract={Glaucoma is a group of clinically relevant eye diseases that eventually result in damage to the retina and optic nerve, leading to vision loss. While it is the main cause of irreversible blindness worldwide, it typically remains asymptomatic until its late stages. Glaucoma is usually diagnosed during routine eye examination, which includes fundoscopic evaluation. Here, we describe our approach for glaucoma detection in the Artificial Intelligence for Robust Glaucoma Screening challenge based on ocular fundus images. We first use object detection to focus on the most relevant part of the image and then use an ensemble of classification networks. Ungradability is rated by combining the reliability score during object detection with an explicit rating of an ungradability neural network. We achieve a sensitivity of detecting glaucoma of 0.8396 at 95% specificity and an area under the curve of 0.9852 for ungradability detection.},
  keywords={Optical losses;Sensitivity;Neural networks;Object detection;Retina;Optical imaging;Reliability;ophthalmology;glaucoma detection;airogs challenge},
  doi={10.1109/ISBIC56247.2022.9854758},
  ISSN={},
  month={March},}@INPROCEEDINGS{10087482,
  author={Tiwari, Shamik and Shukla, Amar and Jain, Anurag and Alferaidi, Ali},
  booktitle={2023 International Conference on Smart Computing and Application (ICSCA)}, 
  title={Broad Analysis of Deep Learning Techniques for Diabetic Retinopathy Screening}, 
  year={2023},
  volume={},
  number={},
  pages={1-5},
  abstract={Retinopathy or Diabetic Retinopathy (DR) is caused by diabetes and leads to vision-impairing retinal lesions. It can cause blindness if left untreated. Treatment is not therapeutic; it only delays eyesight degradation. Early detection and treatment can dramatically lower the chances of occurring. Opthalmologists devote a significant amount of time to manually assessing DR retinal fundus images. Computer vision and artificial intelligence have lately been used to automate the identification of DR. The paper highlights the importance of deep learning (DL) in the diagnosis and detection of DR. Discussion of the many image modalities and the various trends in data sets used to identify DR are also elaborated in detail. After the discussion of the various deep learning-based methods for detecting diabetic retinopathy, recommendations and difficulties for future diabetic retinopathy screening are also discussed in detail.},
  keywords={Deep learning;Training;Retinopathy;Machine vision;Blindness;Tomography;Predictive models;Retinopathy Imaging;Optical coherence tomography (OCT) imaging;Deep Learning;Diabetic Retinopathy Diagnosis},
  doi={10.1109/ICSCA57840.2023.10087482},
  ISSN={},
  month={Feb},}@INPROCEEDINGS{10139437,
  author={Das, Pranajit Kumar and Pumrin, Suree},
  booktitle={2023 Joint International Conference on Digital Arts, Media and Technology with ECTI Northern Section Conference on Electrical, Electronics, Computer and Telecommunications Engineering (ECTI DAMT & NCON)}, 
  title={CNN Transfer Learning for Two Stage Classification of Diabetic Retinopathy using Fundus Images}, 
  year={2023},
  volume={},
  number={},
  pages={443-447},
  abstract={Diabetic retinopathy is a common retina disease caused by diabetes, which is increasing rapidly worldwide along with diabetic Mellitus. It is very difficult to diagnose in the beginning as it is asymptomatic, which leads to blindness. Presently, there are about 422 million diabetes patients, and it is projected that the number will be 552 million in 2030, as per a report from the World Health Organization. With the advancement in Artificial Intelligence, Deep Learning, and Computer Vision fields, many CNN-powered models have been developed for the detection and classification of DR using color fundus images. Early diagnosis of DR will increase recovery and decrease the possibility of vision loss threats. In this study, we aim to classify Diabetic Retinopathy in two stages, diseased versus healthy images. Three different pre-trained CNN models, namely, VGG16, InceptionV3, and MobileNet were deployed through transfer learning. Messidor and Messidor-2, two publicly available datasets are used in the training and testing of these CNN models. In terms of classification accuracy, the highest result of 84% was obtained using InceptionV3, whereas MobileNet and VGG16 shows 83% and 78% accuracy, respectively. The Highest 86 % precision for the healthy class and 88% sensitivity for the diseased class is shown by VGG16.},
  keywords={Training;Image color analysis;Transfer learning;Visual impairment;Retina;Diabetes;Convolutional neural networks;Diabetic Retinopathy;MobileNet;InceptionV3;VGG16;Transfer Learning;CNN;Binary Classification},
  doi={10.1109/ECTIDAMTNCON57770.2023.10139437},
  ISSN={2768-4644},
  month={March},}@INPROCEEDINGS{10466034,
  author={Kumar, Kundan and Tomar, Arjit},
  booktitle={2023 International Conference on Advances in Computation, Communication and Information Technology (ICAICCIT)}, 
  title={Diabetes Prediction System Using Machine Learning}, 
  year={2023},
  volume={},
  number={},
  pages={286-291},
  abstract={Diabetes is long-lasting disease to causes a health care crisis , but it is challenging to predict without medical tests According to the International Diabetes Federation, there are currently 537 million people living with diabetes worldwide. By the year 2045, this number is projected to double, reaching 783 million. Diabetes is a disorder triggered by the abnormal increase in blood glucose levels. This excess blood glucose produces the symptoms of urination, inceased thirst, and improved hunger. Diabetes is a significant contributor to various health complications, including blindness, kidney failure, amputations, heart failure, and strokes. When we consume food, our body processes it into sugars, or glucose. Machine learning algorithms can process vast amounts of data in medicine, revealing confidential information. The aim of study to investigate how machine learning algorithms can used to predict disease. Decision Trees, Numpy, Support Vector Machine are used to predict the diabetes using machine learning techniques. All these machine learning methods/algorithms can examine vast amounts of data, enabling medical practitioners to make more cost- effective judgments. By analyzing clinical data, establishing connections among multiple diagnostic variables, building a predictive model, and comparing it to a new dataset, the study found that the Support Vector Machine Classifier predicted diabetes illnesses with 80% accuracy. The study found that employing a larger dataset for train and test can improve the model's accuracy and modify different hyperparameters. The study concludes that artificial intelligence using ML modeling can categorize people with or without diabetes.},
  keywords={Support vector machines;Machine learning algorithms;Machine learning;Predictive models;Vectors;Diabetes;Glucose;Diabetes;Machine Learning;Decision Trees;Support Vector Machine;Diabetes Prediction},
  doi={10.1109/ICAICCIT60255.2023.10466034},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{9982836,
  author={Sbai, Asma and Touil, Abdelali and Oukhouya, Lamya},
  booktitle={2022 IEEE 3rd International Conference on Electronics, Control, Optimization and Computer Science (ICECOCS)}, 
  title={Diabetic retinopathy detection using a pretrained machine learning model}, 
  year={2022},
  volume={},
  number={},
  pages={1-3},
  abstract={One of the possible side effects of diabetes is Diabetic Retinopathy. Early diagnosis of diabetic retinopathy is an important step toward healing and can prevent many patients from potential blindness. To identify diabetic retinopathy, practitioners may need an artificial intelligence tool to provide a second opinion to confirm or refute a diagnosis. In this article we use a pretrained machine learning model to find a solution to the issue of low-quality inputs by implementing, in the pre-processing step, the technique of Contrast Limited Adaptive Histogram Equalization to extract more features especially the blood vessels.},
  keywords={Integrated optics;Adaptation models;Histograms;Retinopathy;Computational modeling;Machine learning;Feature extraction;Diabetic Retinopathy;machine learning;pretrained model;abnormalities detection;fundus images;computer-aided diagnosis},
  doi={10.1109/ICECOCS55148.2022.9982836},
  ISSN={},
  month={Dec},}@ARTICLE{10494868,
  author={Balla, Goutam and Hashmi, Mohammad Farukh and Kotambkar, Deepali M.},
  journal={IEEE Sensors Letters}, 
  title={Selective Routing-Based Optic Disc Segmentation for Glaucoma Diagnosis}, 
  year={2024},
  volume={8},
  number={5},
  pages={1-4},
  abstract={Glaucoma is one of the most critical retinal diseases that can cause irreversible blindness if not treated in time. The primary detection of this disease is usually done based on the diameter measurement of optic disc (OD) region in a color fundus image. Many artificial intelligence based expert systems were developed to automatically segment the OD region of a given fundus image. Most of the recent research in this direction employs a variant of convolution neural network based U-Net architecture, which substantially increases the computational load on the training system. Also, these methods are invariant to the pose, orientation, and spatial relationship of OD to other lesions in a given fundus image. In this letter, we propose a novel lightweight W-capsule network, which uses capsules for semantic segmentation through selective routing. The instantaneous vectors of the W-shaped structure at the bottom of the network accurately extract the shape of OD along with its orientation and spatial relationship to other fundus biomarkers, making it unique in compensating for variations due to image acquisition settings. The model was trained and validated on the Indian diabetic retinopathy image dataset, DRISHTI, and RIM-ONE datasets and achieved near state-of-the-art accuracy (acc) of 0.995 and intersection over union score of 0.931 with only 2.68M computational parameters, which is only 8.64% of most widely used U-Net-based segmentation structure.},
  keywords={Routing;Optical imaging;Image segmentation;Optical sensors;Computer architecture;Signal processing;Artificial intelligence;Computer vision;Deep learning;Biomedical image processing;Retina;Medical diagnosis;Sensor signal processing;artificial intelligence (AI);computer vision;deep learning;medical image analysis;retinal disease diagnosis;sensor signal processing},
  doi={10.1109/LSENS.2024.3384679},
  ISSN={2475-1472},
  month={May},}@INPROCEEDINGS{10582129,
  author={S, Sivagami and M, Gopinath and J, Yuktha Varshika and V, Srivarshinie and R, Monikaa and K, Senthil},
  booktitle={2024 International Conference on Advances in Modern Age Technologies for Health and Engineering Science (AMATHE)}, 
  title={Visionary Insights: CNN-Driven Retinal Vessel Segmentation in Ophthalmic Imaging}, 
  year={2024},
  volume={},
  number={},
  pages={1-7},
  abstract={Retinal vessel segmentation plays a vital role in the analysis of ophthalmic imaging. Binary Segmentation is one type of segmentation that segments the retinal vessel and blood vessel in retinal images. Retinal vascular geometry reflects a patient's state of health and aids in the diagnosis of certain conditions, including hypertension and diabetes. Early detection of these diseases helps to prevent blindness of patients. Machine Learning and Deep Learning are two important sectors in Artificial Intelligence. Nowadays, Deep learning algorithms are very much used in Medical Image processing. These Deep Learning Algorithms are more accurate and efficient than manual segmentation and other computer-aided diagnosis methods. Especially Convolutional Neural Networks (CNNs) in Deep Learning Architecture are used for the segmentation process. CNN architecture can handle the complex characteristics of retinal images. Traditional Data augmentation techniques are used to enlarge the training and testing samples. Matched filter methods are used to remove the noise and Morphological operations are implemented to detect the abnormal regions in the retinal image. Our proposed model achieves an accuracy of 97.87%, precision of 96.8%, Intersection over Union of 97.1%, and specificity of 0.5% in the segmentation process. The proposed CNN architecture not only expedites the segmentation process but it makes advancements in clinical decisions in the field of ophthalmology.},
  keywords={Deep learning;Image segmentation;Matched filters;Machine learning algorithms;Accuracy;Computer architecture;Data augmentation;Retinal vessel segmentation;Ophthalmic imaging;Convolutional Neural Network;Matched filter methods;Morphological segmentation},
  doi={10.1109/AMATHE61652.2024.10582129},
  ISSN={},
  month={May},}@INPROCEEDINGS{10981490,
  author={Azimov, Shawn and Delgado, Jordi and Romero, Enrique and Vellido, Alfredo and Zarranz-Ventura, Javier and König, Caroline},
  booktitle={2025 IEEE Symposium on Trustworthy, Explainable and Responsible Computational Intelligence (CITREx Companion)}, 
  title={CNN Layer-wise explanations for the prediction of Diabetic Retinopathy from OCTA images}, 
  year={2025},
  volume={},
  number={},
  pages={1-5},
  abstract={Optical Coherence Tomography Angiography (OCTA) is a promising new imaging technique in ophthalmology for diagnosing Diabetic Retinopathy (DR) in diabetic patients. Early detection and treatment of DR is crucial to prevent vision loss and blindness. Deep Learning-based approaches to image classification have proven to be very effective in image analysis tasks, but their adoption hinges on the ability of the medical professionals to understand how these networks make their decisions. This study focuses on exploring explainable artificial intelligence techniques to better understand how a Convolutional Neural Network (CNN) classifies OCTA images of diabetic patients with varying levels of DR. This exploration is based on Signature Activation, a technique that generates holistic and class-agnostic explanations of the CNN’s decisions.},
  keywords={Optical losses;Diabetic retinopathy;Optical coherence tomography;Fasteners;Ophthalmology;Convolutional neural networks;Artificial intelligence;Medical diagnostic imaging;Computational intelligence;Image classification;diabetic retinopathy;explainable artificial intelligence;signature activation;convolutional neural network},
  doi={10.1109/CITRExCompanion65208.2025.10981490},
  ISSN={},
  month={March},}@INPROCEEDINGS{10652422,
  author={A, Sai Aasrith and G, Loknath Sai and M, Monica Subashini and I, Jacob Raglend},
  booktitle={2024 IEEE Students Conference on Engineering and Systems (SCES)}, 
  title={A Deep Learning Model for Diagnosing Diabetic Retinopathy Using EfficientNet-B0}, 
  year={2024},
  volume={},
  number={},
  pages={1-5},
  abstract={Diabetic Retinopathy (DR) is a severe ocular condition prevalent among individuals with diabetes, which can result in vision loss and ultimately blindness. This research presents an innovative approach to identify Diabetic Retinopathy (DR) using a Convolutional Neural Network (CNN) model known as EfficientNet-B0. Convolutional neural networks (CNNs), due to their capacity to efficiently analyze images, have a pivotal function in the diagnosis of diabetic retinopathy (DR). The model was trained using eye pictures from diabetic patients, depicting various phases of diabetic retinopathy (DR). The CNN successfully extracted significant information from the photos, allowing our model to reliably identify these stages. Our research demonstrates the efficacy of artificial intelligence, particularly convolutional neural networks (CNNs), in facilitating the timely identification and management of diabetic retinopathy (DR). The favorable outcomes derived from our model indicate its potential as a valuable instrument for ophthalmologists and promote additional investigation in this field.},
  keywords={Deep learning;Diabetic retinopathy;Adaptation models;Instruments;Blindness;Convolutional neural networks;Reliability;diabetic retinopathy (DR);convolutional neu ral network (CNN);EfficientNet-B0;artificial intelligence (AI);ophthalmologists},
  doi={10.1109/SCES61914.2024.10652422},
  ISSN={},
  month={June},}@INPROCEEDINGS{10442012,
  author={Hu, Zhiyuan and He, Xiaoran and Lin, Yangde and Zhao, Yuxuan and Liang, Zijuan and Wu, Jiahong},
  booktitle={2023 IEEE International Conference on Electrical, Automation and Computer Engineering (ICEACE)}, 
  title={Intelligent Point Reader Based on Machine Vision and Speech Processing}, 
  year={2023},
  volume={},
  number={},
  pages={975-983},
  abstract={With the rapid development of artificial intelligence technology, human-computer interaction has also undergone tremendous changes. The development of interactive mode has brought great convenience and excellent experience to people's operation. At the same time, smart education has become the golden track of China's artificial intelligence market in 2021. The development of artificial intelligence online education industry is gradually accelerating, and the demand for learning machines tends to be just needed. The team has developed a “point” music-intelligent point reader based on machine vision and speech processing. Based on a variety of recognition modes, this product independently designs algorithms, conducts reasonable hardware combination, integrates biosensor, machine vision, python, OCR, object detection, raspberry pie, and adds humanized reminder modules to achieve diverse and intelligent reading functions. Through our products, users can realize the learning and expansion of unfamiliar knowledge, the learning of multiple languages and so on. It can not only help users learn English and other foreign languages, but also provide convenience for blind people to read. The application scenarios are rich and diversified. It can not only identify which word by clicking on it, but also identify it by drawing a module on the book by hand, so as to make learning more efficient and convenient. This product proposes a new type of human-computer interaction that can be landed. It will be independently applied to the system, with gesture sensing, resonance rate, operation setting, human gesture recognition, trigger channel and other functions. It has the characteristics of accurate detection, efficient management and human structure.},
  keywords={Industries;Machine vision;Optical character recognition;Educational technology;Hardware;Artificial intelligence;Speech processing;OpenCV;Human-computer interaction;OCR;Point reading machine},
  doi={10.1109/ICEACE60673.2023.10442012},
  ISSN={},
  month={Dec},}@INPROCEEDINGS{10382327,
  author={Phu, Dang Nguyen and Tran, Anh Quang and Tran, Ngoc Quang and Dang, Tran Huy},
  booktitle={2023 12th International Conference on Control, Automation and Information Sciences (ICCAIS)}, 
  title={The classification of optical coherence tomography images using machine learning for macular hole and diabetic macular edema diagnoses}, 
  year={2023},
  volume={},
  number={},
  pages={634-638},
  abstract={Optical Coherence Tomography is a non-invasive medical imaging method used to create images of the retina in eyes, which can be vital to diagnose visual health and diseases. Retinal diseases can partially damage the retina, causing visual problem and even blindness. Therefore, early and precise detection of retinal diseases is crucial to avoid vision deterioration. In recent years, artificial intelligence algorithms have been increasingly developed to automatic identification and classification of medical images that help doctors to improve the accuracy of disease diagnosis. In this work, we propose a simple and effective model of automatic classification for Macular hole and Diabetic macular edema diseases based on optical coherence tomography images by using the Support Vector Machine. Our proposed model is also evaluated by different training data and real images. The high accuracy values of macular hole and diabetic macular edema classifications are up to 95.8% and 86.7% respectively. Our obtained results can be used to effectively assist doctors in the early diagnosis and treatment of such retinal diseases.},
  keywords={Training;Visualization;Optical interferometry;Optical coherence tomography;Training data;Machine learning;Retina;Optical Coherence Tomography;Machine learning;Support Vector Machine;Michelson interferometer;Macular hole;Diabetic macular edema},
  doi={10.1109/ICCAIS59597.2023.10382327},
